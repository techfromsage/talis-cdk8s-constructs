    "~UCBrowser\\/1[01]\\.\\d+\\.\\d+[\\.\\d]* "                                                                 "ucbrowser_before_12_13_2"; # version 10/11
    "~UCBrowser\\/12\\.\\d\\.\\d+[\\.\\d]* "                                                                     "ucbrowser_before_12_13_2"; # version 12.0 - 12.9
